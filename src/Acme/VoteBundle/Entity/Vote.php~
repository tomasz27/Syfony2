<?php

namespace Acme\VoteBundle\Entity;

use Doctrine\ORM\Mapping as ORM;


/**
 * @ORM\Entity
 * @ORM\Table()
 */
class Vote
{
    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @ORM\Column(type="integer")
     */
    protected $alertId;

    /**
     * @ORM\OneToOne(targetEntity="Alert")
     * @ORM\Column(type="integer")
     */
    protected $vote;

}
